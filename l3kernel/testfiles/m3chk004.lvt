    \TYPE{INT:     \int_use:c      {l_A_int}       }\SEPARATOR
    \TYPE{DIM:     \dim_use:N      \l_A_dim        }\SEPARATOR
    \TYPE{DIM:     \dim_use:c      {l_A_dim}       }\SEPARATOR
    \TYPE{SKIP:    \skip_use:N     \l_A_skip       }\SEPARATOR
