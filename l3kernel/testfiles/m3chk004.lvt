\begin{document}

\START
\AUTHOR{Will Robertson}
\ExplSyntaxOn

\OMIT
\cs_new:Npn \cs_aaa: {}
\tl_new:N  \l_A_tl
\str_new:N  \l_A_str
%% there is no \quark_use:N :)
