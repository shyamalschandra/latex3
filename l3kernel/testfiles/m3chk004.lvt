    \TYPE{FP:      \fp_use:N       \l_A_fp         }\SEPARATOR
    \TYPE{FP:      \fp_use:c       {l_A_fp}        }\SEPARATOR
  }

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

\TEST { Use~ variables~ (undefined) }
  {
    \TYPE{CS:      \cs_use:N       \cs_bbb:        }\SEPARATOR
    \TYPE{CS:      \cs_use:c       {cs_bbb:}       }\SEPARATOR
    \TYPE{TL:      \tl_use:N       \l_B_tl         }\SEPARATOR
